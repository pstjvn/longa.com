      "$_name": "lastName"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "pp_email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "cpassword": {
      "type": "string",
