/** Template for the recovery of an account */
{template .RecoverForm}
  <div is class="{css longa-recover-form}" use-pointer>
    <div>
      {call .ErrorMessage /}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error: 'Valid email is required' /}
        {param required: true /}
        {param label: 'E-mail' /}
        {param name: 'email' /}
        {param type: 'email' /}
        {param value: '' /}
      {/call}
    </div>
    <div class="{css button-container}">
      {call pstj.material.template.Button}
        {param content: 'request password' /}
        {param icon: 'none' /}
        {param ink: true /}
      {/call}
    </div>
    <div>
      <span class="{css linklike}" data-request="login">Go back to login</span>
    </div>
    <div is class="{css material-shadow} {css material-shadow-2}">
      <div class="{css material-shadow-bottom}"></div>
      <div class="{css material-shadow-top}"></div>
    </div>
  </div>
{/template}


