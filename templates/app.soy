    </div>
    <div is class="{css material-shadow} {css material-shadow-2}">
      <div class="{css material-shadow-bottom}"></div>
      <div class="{css material-shadow-top}"></div>
    </div>
  </div>
{/template}


/** The start screen of the app */
{template .StartScreen}
<div class="{css longa-start-screen}" fit>
  <div is class="{css pstj-swiper} {css core-swipe}" use-pointer>
    {call pstj.templates.Swipetile}
      {param src: 'assets/images/bull.jpg' /}
      {param text kind="text"}
        {msg desc="swiper tile title"}The ultimate stock signal marketplace{/msg}
      {/param}
    {/call}
    {call pstj.templates.Swipetile}
      {param src: 'assets/images/b.jpg' /}
      {param text kind="text"}
        {msg desc="swiper tile title"}Where investors meet successful signal traders{/msg}
      {/param}
    {/call}
    {call pstj.templates.Swipetile}
      {param src: 'assets/images/c.jpg' /}
      {param text kind="text"}
        {msg desc="swiper tile title"}Make money from stock investments{/msg}
      {/param}
    {/call}
    {call pstj.templates.Swipetile}
      {param src: 'assets/images/hands.jpg' /}
      {param text kind="text"}
        {msg desc="swiper tile title"}Follow the best investment strategy{/msg}
      {/param}
    {/call}
    {call pstj.templates.Swipetile}
      {param src: 'assets/images/d.jpg' /}
      {param text kind="text"}
        {msg desc="swiper tile title"}Sell your trading ideas and make money{/msg}
      {/param}
    {/call}
  </div>
  <div class="{css longa-start-screen-top}">
    <div class="{css longa-start-screen-logo}">
      {call .LongaLogo}
        {param width: '100px' /}
        {param height: '40px' /}
      {/call}
    </div>
    <div class="{css longa-start-screen-up-button}">
      {call pstj.material.template.Button}
        {param ink: true /}
        {param content kind="text"}{msg desc="button label"}Skip{/msg}{/param}
        {param icon: 'none' /}
      {/call}
    </div>
  </div>
  <div class="{css longa-start-screen-rotation-indicator}">
    <div is class="{css material-radio-group} {css material-radio-group-disabled}">
      {call pstj.material.template.RadioButton /}
      {call pstj.material.template.RadioButton /}
      {call pstj.material.template.RadioButton /}
      {call pstj.material.template.RadioButton /}
      {call pstj.material.template.RadioButton /}
    </div>
  </div>
  <div class="{css longa-start-screen-buttons}">
    {call pstj.material.template.Button}
      {param ink: true /}
      {param content kind="text"}{msg desc="button label"}Sign up{/msg}{/param}
      {param icon: 'none' /}
    {/call}
    {call pstj.material.template.Button}
      {param ink: true /}
      {param content kind="text"}{msg desc="button label"}Log in{/msg}{/param}
      {param icon: 'none' /}
    {/call}
  </div>
</div>
{/template}


/** The main app template */
{template .MainApp}
  <div is class="{css longa-main-app}"></div>
