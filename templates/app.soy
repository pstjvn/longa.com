    <div is class="{css material-fab} {css material-fab-transition} {css material-fab-raised} {css material-fab-tactile} {css longa-app-buy-credit-back-button}" ink action="goback" use-pointer icon="back-arrow">
      {call pstj.material.template.Shadow /}
      {call pstj.material.template.IconContainer}
        {param type: 'back-arrow' /}
      {/call}
      {call pstj.material.template.Ripple}
        {param recenter: true /}
      {/call}
    </div>
  </div>
{/template}

/** Themplate for the Pages component. */
{template .Balance}
  <div is class="{css longa-app-balance-pages}" animate>
    <div is class="{css longa-app-page} {css animation-left-right}" style="overflow-y: auto;">
    </div>
    <div is class="{css longa-app-page} {css animation-right-left}">
      {call .ExchangeForm /}
    </div>
    <div is class="{css longa-app-page} {css animation-right-left}">
      {call .ExchangeForm}
        {param withdraw: true /}
      {/call}
    </div>
  </div>
{/template}
