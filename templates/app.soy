/** Toast for new alerts. */
{template .ToastForNewAlerts}
  {msg desc="toast message for new alerts"}New alerts received!{/msg}
{/template}

/** Button label */
{template .ToastForNewAlertsButton}
  {msg desc="button label"}Go to alerts{/msg}
{/template}

/** Button label */
{template .ShowNewAlerts}
  {msg desc=""}Show new alerts{/msg}
{/template}

/** Message when login with existing credentials from storgae did not work. */
{template .CannotLoginWithSavedCredentials}
  {msg desc="error message"}Could not login, please re-enter your credentials{/msg}
{/template}

/** Text template */
{template .DefaultToastButton}
  {msg desc="The default text to put on toasts"}dismiss{/msg}
{/template}

/** Templetize the string used in code. */
{template .StringFAQLabelForUsers}
  {msg desc=""}Go to signal feed{/msg}
{/template}

/** Templetize the string used in code. */
{template .StringFAQLabelForSeller}
  {msg desc=""}Go to service definition{/msg}
{/template}

/** Another template */
{template .Address}
  {@param city: string}
  <div>Lives in {$city}</div>
{/template}

/** Default error message template */
{template .ErrorMessage}
  <div is class="{css longa-error-message}"></div>
{/template}

/** Doc */
{template .LoginForm}
  <div is class="{css longa-login-form}" use-pointer>
    <div>
      {call .ErrorMessage /}
    </div>
    <div class="{css linklike}" style="display: none;" data-request="recovery">
      {msg desc="Error displayed to user in form"}Forgot your password?{/msg}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Username is required{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Username{/msg}{/param}
        {param required: true /}
        {param name: 'username' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Password is required{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Password{/msg}{/param}
        {param required: true /}
        {param name: 'password' /}
        {param type: 'password' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Checkbox}
        {param content kind="text"}
          {msg desc="label on checkbox"}remember me{/msg}
        {/param}
      {/call}
    </div>
    <div class="{css help-info}">{msg desc="hint for checkbox"}Stay logged in{/msg}</div>
    <div class="{css button-container}">
      {call pstj.material.template.Button}
        {param content kind="text"}
          {msg desc="button label"}login{/msg}
        {/param}
        {param icon: 'none' /}
        {param ink: true /}
      {/call}
    </div>
    <div>
      <span class="{css linklike}" data-request="register">
        {msg desc="Link-like clickable hint in form"}Or register a new account{/msg}
      </span>
    </div>
    <div is class="{css material-shadow} {css material-shadow-2}">
      <div class="{css material-shadow-bottom}"></div>
      <div class="{css material-shadow-top}"></div>
    </div>
  </div>
{/template}


/** Registration form for Longa.com*/
{template .RegistrationForm}
  {@param registration: bool}
  <div is class="{css longa-registration-form}" use-pointer>
    {if $registration}
      <div>
        <span class="{css linklike}" data-request="login">
          {msg desc="Link-like clickable note to user"}Go back to login{/msg}
        </span>
      </div>
      <label>{msg desc=""}User type{/msg}:</label>
      <div is class="{css material-radio-group}" value="1">
        {call pstj.material.template.RadioButton}
          {param content kind="text"}
            {msg desc="label on radio button - investor"}Investor{/msg}
          {/param}
          {param name: 'type' /}
          {param value: '1' /}
        {/call}
        {call pstj.material.template.RadioButton}
          {param content kind="text"}
            {msg desc="label on radio button - signal provider"}Signal provider{/msg}
          {/param}
          {param name: 'type' /}
          {param value: '2' /}
        {/call}
      </div>
      <div>
        {call pstj.material.template.Input}
          {param label kind="text"}{msg desc="input hint/placeholder"}Username{/msg}{/param}
          {param error kind="text"}
            {msg desc="Error when username is not filled in"}Username is required' {/msg}
          {/param}
          {param required: true /}
          {param name: 'username' /}
          {param type: 'text' /}
          {param value: '' /}
        {/call}
      </div>
    {/if}
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Minimum 6 symbols{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Password{/msg}{/param}
        {{param pattern: '^.{6,50}$'/}}
        {param name: 'password' /}
        {param type: 'password' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Minimum 6 symbols{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Repeat password{/msg}{/param}
        {{param pattern: '^.{6,50}$' /}}
        {param name: 'cpassword' /}
        {param type: 'password' /}
        {param value: '' /}
      {/call}
    </div>
        <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Minimum 4, letters only{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}First name{/msg}{/param}
        {{param pattern: '^[A-Za-z ]{4,50}$' /}}
        {param name: 'fname' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Minimum 4, letters only{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Last name{/msg}{/param}
        {{param pattern: '^[A-Za-z ]{4,50}$' /}}
        {param name: 'lname' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param label kind="text"}{msg desc="input hint/placeholder"}Company{/msg}{/param}
        {param name: 'company' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Valid email is required{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}E-mail{/msg}{/param}
        {param required: true /}
        {param name: 'email' /}
        {param type: 'email' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Valid email only{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}PayPal E-mail{/msg}{/param}
        {param name: 'pp_email' /}
        {param type: 'email' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Please use this format: +0000000..{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}Phone{/msg}{/param}
        {param name: 'phone' /}
        {param type: 'tel' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param label kind="text"}{msg desc="input hint/placeholder"}Address{/msg}{/param}
        {param name: 'address' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param label kind="text"}{msg desc="input hint/placeholder"}City{/msg}{/param}
        {param name: 'city' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      <label>{msg desc=""}State{/msg}:</label>
      <select name="state">
        {call pstj.select.USAStates /}
      </select>
    </div>
    <div>
      <label>{msg desc=""}Country{/msg}:</label>
      <select name="country">
        {call pstj.select.Countries /}
      </select>
    </div>
    <div>
      {call .ErrorMessage /}
    </div>
    <div class="{css button-container}">
      {if $registration}
        {call pstj.material.template.Button}
          {param content kind="text"}
            {msg desc="Register button label"}Register{/msg}
          {/param}
          {param icon: 'none' /}
          {param ink: true /}
        {/call}
      {else}
        {call pstj.material.template.Button}
          {param content kind="text"}
            {msg desc="Update button label"}Update{/msg}
          {/param}
          {param icon: 'none' /}
          {param ink: true /}
        {/call}
      {/if}
    </div>
    <div is class="{css material-shadow} {css material-shadow-2}">
      <div class="{css material-shadow-bottom}"></div>
      <div class="{css material-shadow-top}"></div>
    </div>
  </div>
{/template}


/** Template for the recovery of an account */
{template .RecoverForm}
  <div is class="{css longa-recover-form}" use-pointer>
    <div>
      {call .ErrorMessage /}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error kind="text"}{msg desc="Error on input"}Valid email is required{/msg}{/param}
        {param label kind="text"}{msg desc="input hint/placeholder"}E-main{/msg}{/param}
        {param required: true /}
        {param name: 'email' /}
        {param type: 'email' /}
        {param value: '' /}
      {/call}
    </div>
    <div class="{css button-container}">
      {call pstj.material.template.Button}
        {param content kind="text"}
          {msg desc="button label"}request password{/msg}
        {/param}
        {param icon: 'none' /}
        {param ink: true /}
      {/call}
    </div>
    <div>
      <span class="{css linklike}" data-request="login">
