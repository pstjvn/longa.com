{namespace longa.template}

/** Test our theory about DTO */
{template .Test}
  {@param name: string}
  {@param age: number}
  {@param address: ?}
  <div>
    <div>{$name} - {$age}</div>
    {call .Address data="$address" /}
  </div>
{/template}

/** Another template */
{template .Address}
  {@param city: string}
  <div>Lives in {$city}</div>
{/template}

/** Default error message template */
{template .ErrorMessage}
  <div is class="{css longa-error-message}"></div>
{/template}

/** Doc */
{template .LoginForm}
  <div is class="{css longa-login-form}" use-pointer>
    <div>
      {call .ErrorMessage /}
    </div>
    <div class="{css linklike}" style="display: none;" data-request="recovery">
      Forgot your password?
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error: 'Username is required' /}
        {param required: true /}
        {param label: 'Username' /}
        {param name: 'username' /}
        {param type: 'text' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Input}
        {param error: 'Password is required' /}
        {param required: true /}
        {param label: 'Password' /}
        {param name: 'password' /}
        {param type: 'password' /}
        {param value: '' /}
      {/call}
    </div>
    <div>
      {call pstj.material.template.Checkbox}
        {param content: 'remember me' /}
      {/call}
    </div>
    <div class="{css help-info}">Stay logged in</div>
    <div class="{css button-container}">
      {call pstj.material.template.Button}
        {param content: 'login' /}
        {param icon: 'none' /}
        {param ink: true /}
      {/call}
    </div>
    <div>
      <span class="{css linklike}" data-request="register">Or register a new account</span>
    </div>
    <div is class="{css material-shadow} {css material-shadow-2}">
      <div class="{css material-shadow-bottom}"></div>
      <div class="{css material-shadow-top}"></div>
    </div>
  </div>
{/template}


/** Registration form for Longa.com*/
{template .RegistrationForm}
